{
  "name": "SmartMat",
  "productName": "SmartMat",
  "version": "1.0.0",
  "description": "A tablet electron app to track mats!",
  "main": "main.js",
  "scripts": {
    "electron": "electron .",
    "start": "webpack-dev-server",
    "dist": "build  --win --ia32"
  },
  "build": {
    "appId": "gems.smartmat",
    "extraResources": [
      {
        "from": "app/yuma/yuma-lib",
        "to": "yuma-lib"
      },
      {
        "from": "app/settings/settings.json",
        "to": "settings.json"
      },
      {
        "from": "app/data/clients.json",
        "to": "clients.json"
      },
      {
        "from": "app/data/scans.json",
        "to": "scans.json"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/images/icon.ico"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^10.1.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "getmac": "^1.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.1.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.77.1",
    "tcp-ping": "^0.1.1",
    "wifi-control": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengzhou44/smartmat-tablet.git"
  },
  "bugs": {
    "url": "https://github.com/mengzhou44/smartmat-tablet/issues"
  },
  "homepage": "https://github.com/mengzhou44/smartmat-tablet#readme"
}
