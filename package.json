{
  "name": "yumagps",
  "productName": "Yuma GPS",
  "version": "1.0.0",
  "description": "A tablet electron app to get GPS data from YUMA!",
  "main": "main.js",
  "scripts": {
    "electron": "electron .",
    "start": "webpack-dev-server",
    "dist": "build  --win --ia32"
  },
  "build": {
    "appId": "yuma.gps",
    "extraResources": {
      "from": "app/yuma-lib",
      "to": "yuma-lib"
    },
    "win": {
      "target": "nsis"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^10.1.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "tcp-ping": "^0.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengzhou44/smartmat-tablet.git"
  },
  "bugs": {
    "url": "https://github.com/mengzhou44/smartmat-tablet/issues"
  },
  "homepage": "https://github.com/mengzhou44/smartmat-tablet#readme"
}
